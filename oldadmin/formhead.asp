<%
var clubname;
var pagetitle;
clubname = "Hampton-In-Arden Tennis Club";
%>
<!doctype html public "-//w3c//dtd html 3.2//en">
<html>
	<head>
		<title><%= clubname %> - <%= pagetitle %></title>
		<meta name="GENERATOR" content="Microsoft FrontPage 4.0">
		<meta name="FORMATTER" content="Arachnophilia 4.0">
		<link rel=STYLESHEET type="text/css" href="tennis.css">
	</head>
	<body bgcolor="#ffffff" text="#000000" link="#0000ff" vlink="#800080" alink="#ff0000">
		<div align="left">
			<table width="650" border="0" cellspacing="0" cellpadding="0">
				<tr>
					<td class="tdfont" align="LEFT" valign="TOP">
						<hr width=550 size=10 color="black" noshade>
						<h3><%= clubname %><BR><%= pagetitle %></h3>
					</td>
					<td class="tdfont" valign="TOP" align="CENTER" width="110" nowrap>
						<img src="images/logo.gif" width="88" height="100" alt="Club Logo">
					</td>
				</tr>
			</table>
		</div>
